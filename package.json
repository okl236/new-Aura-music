{
  "name": "music-player",
  "version": "1.0.0",
  "description": "Music player with MusicFree plugin support",
  "main": "electron-main.js",
  "scripts": {
    "start": "node server.js",
    "install-deps": "npm install",
    "dev": "electron .",
    "build:electron": "electron-builder --win portable",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "he": "^1.2.0",
    "crypto-js": "^4.2.0",
    "node-fetch": "^2.7.0",
    "big-integer": "^1.6.51",
    "webdav": "^5.6.0",
    "qs": "^6.11.2"
  },
  "devDependencies": {
    "electron": "^33.0.0",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.aura.music.player",
    "productName": "Aura Music Player",
    "directories": {
      "output": "../4242"
    },
    "files": [
      "**/*",
      "!dist/**",
      "!node_modules/.cache/**"
    ],
    "win": {
      "icon": "../1231.ico",
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}
